{
  "name": "sql_iot",
  "version": "1.0.0",
  "description": "Server packages for an http server with mysql",
  "main": "server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run compile && node ./build/server.js",
    "clean": "npx tsc --build --clean ./source",
    "compile": "tsc --project ./source",
    "build": "npm run clean && npm run compile",
    "test": "npm run compile && node --test",
    "lint": "eslint . --ext .ts",
    "dev": "concurrently --kill-others \"nodemon\" \"npx rollup --config ./source_client/rollup.config.js --watch\"",
    "db-drop": "npm run build && node ./build/models/database_clear.js",
    "db-create": "npm run build && node ./build/models/database_seed.js",
    "db-seed": "npm run build && node ./build/models/database_seed.js --seed",
    "db-dev-reset": "npm run db-drop && node ./build/models/database_seed.js --seed"
  },
  "nodemonConfig": {
    "watch": [
      "source",
      "config.toml"
    ],
    "ext": "ts",
    "delay": "200",
    "execMap": {
      "ts": "ts-node"
    },
    "exec": "npm --trace-warnings run start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.2",
    "@types/js-cookie": "^3.0.3",
    "@types/multer": "^1.4.9",
    "@types/sequelize": "^4.28.14",
    "@types/socket.io-client": "^3.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.8",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "jquery": "^3.6.4",
    "js-cookie": "^3.0.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.1.2",
    "node-windows": "^1.0.0-beta.8",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^3.21.6",
    "rollup-plugin-scss": "^4.0.0",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "toml": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^11.0.0",
    "@types/jquery": "^3.5.16",
    "@types/node": "^18.16.2",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "copyfiles": "^2.4.1",
    "cypress": "^13.3.3",
    "eslint": "^8.39.0",
    "nodemon": "^3.0.1",
    "sass": "^1.62.1",
    "semver": "^7.5.4",
    "supertest": "^6.3.3"
  }
}
